document.addEventListener("DOMContentLoaded", onko kirjautunut); 

function onkoKirjautunut(){
    let kirjautunut = "kylla"
    if (kirjautunut === "kylla"){
        document.getElementById("tervetuloa_teksti").textContent += "Johanna!";
    }
}
function kirjaudu(){
    localStorage.setItem("nimi", document.getElementById(”nimi”).value);

}